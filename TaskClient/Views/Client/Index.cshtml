@Model.ClientList
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<a class="nav-link text-dark" asp-area="" asp-controller="Client" asp-action="Update" asp-route-id="0">New</a>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Address</th>
            <th>Phone Number</th>
            <th>CUIT</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Client in Model.ClientList)
        {
            <tr>
                <td>@Client.Id</td>
                <td>@Client.Name</td>
                <td>@Client.Description</td>
                <td>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Client" asp-action="Update" asp-route-id="@Client.Id">Update</a>
                </td>
            </tr>
        }
    </tbody>
</table>